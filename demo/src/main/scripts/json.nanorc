syntax "JSON" "\.json$"
header "^\{$"

NUMBER:   "\<[-]?[0-9]*([Ee][+-]?[0-9]+)?\>"  "\<[-]?[0](\.[0-9]+)?\>"
NULL:     "\<null\>"
BOOLEAN:  "\<(true|false)\>"
STRING:   ""(\\.|[^"])*"|'(\\.|[^'])*'"
VARIABLE: "\"(\\"|[^"])*\"[[:space:]]*:"  "'(\'|[^'])*'[[:space:]]*:"
ESCAPE:   "\\u[0-9a-fA-F]{4}|\\[bfnrt'"/\\]"
