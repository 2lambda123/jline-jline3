syntax "ARGS"

NUMBER:    "\<[-]?[0-9]*([Ee][+-]?[0-9]+)?\>"  "\<[-]?[0](\.[0-9]+)?\>"
STRING:    ""(\\.|[^"])*"|'(\\.|[^'])*'|[a-zA-Z]+[a-zA-Z0-9]*"
FUNCTION:  "\<(console|grab|inspect)\>"
NULL:      "\<null\>"
BOOLEAN:   "\<(true|false)\>"
VARIABLE:  "\"(\\"|[^"])*\"\s*:"  "'(\'|[^'])*'\s*:" "(\[|,)\s*[a-zA-Z0-9]*\s*:"
PLAIN:     "(:|\[|,|\])"
ESCAPE:    "\\u[0-9a-fA-F]{4}|\\[bfnrt'"/\\]"
COMMENT:   start="/\*" end="\*/"
COMMENT:   "(//.*)"
